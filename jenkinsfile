pipeline {
    agent any

     parameters {
        
        string(name: "STACK_NAME", defaultValue: "ssbostan", trim: true, description: "Sample string parameter")
        string(name: "KEY", defaultValue: "ssbostan", trim: true, description: "Sample string parameter")
        string(name: "VALUE", defaultValue: "ssbostan", trim: true, description: "Sample string parameter")
       
    }
    stages {
        stage('Build') {
            steps {
                
                script {
                    
                   cf_change_parameters.py $STACK_NAME $KEY $VALUE
                  
                }
                    
                 
            }
        }
    }
}